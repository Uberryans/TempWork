<Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Equation executable="true" style="2D Input" input-equation="" display="LUklbXJvd0c2Iy9JK21vZHVsZW5hbWVHNiJJLFR5cGVzZXR0aW5nR0koX3N5c2xpYkdGJzYlLUkjbWlHRiQ2I1EhRicvJStleGVjdXRhYmxlR1EmZmFsc2VGJy8lLG1hdGh2YXJpYW50R1Enbm9ybWFsRic=">JSFH</Equation></Text-field>
</Input>
</Group><CodeEditor-ExecGroup view="presentation" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion0" visible="true" pixel-width="705" pixel-height="208" code-language="text/maple" autoexpand="false" wrapping="false" show-border="true" code-line-numbers="true">my_trs := proc(term)
	local a, b;
	return applyrule([
		#Starter Rules
		plus(S(a::algebraic),b::algebraic) = S(plus(a,b)),
		plus(neg(S(a::algebraic)),b::algebraic) = P(plus(neg(a), b)),
		plus(0, b::algebraic) = b,
		plus(neg(0), b::algebraic) = b,
		mult(0, b::algebraic) = 0,
		mult(S(a::algebraic),b::algebraic) = plus(b, mult(a, b)),
		mult(neg(S(a::algebraic)),b::algebraic) = neg(plus(b, mult(a,b))),
		mult(neg(0), b::algebraic) = 0,
		sub(S(a::algebraic),b::algebraic) = P(sub(a, b)),
		sub(neg(S(a::algebraic)),b::algebraic) = S(sub(neg(a),b)),
		sub(0, b::algebraic) = b,
		sub(neg(0), b::algebraic) = b
		], term);
end proc;

convert_to_number := proc(term)
	local s,p;
	s := x -&gt; x+1;
	p := x -&gt; x-1;
	return eval(term, [S=s, P=p]);
end proc:

convert_from_number := proc(term)
	if term = 0 then
		return 0;
	end if;
	if term &gt; 0 then
		return S(convert_from_number(term-1));
	end if;
	if term &lt; 0 then
		return P(convert_from_number(term+1));
	end if;
end proc:

term := sub(neg(S(S(0))), S(S(0)));
test := my_trs(term);
convert_to_number(test);</EC-CodeEditor>
